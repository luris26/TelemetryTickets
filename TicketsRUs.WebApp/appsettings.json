{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Debug": {
    "LogLevel": {
        "Default": "Information"
    }
},
"Console": {
    "IncludeScopes": true,
    "LogLevel": {
        "Microsoft.Extensions.Hosting": "Warning",
        "Default": "Information"
    }
},
"EventSource": {
    "LogLevel": {
        "Microsoft": "Information"
    }
},
"EventLog": {
    "LogLevel": {
        "Microsoft": "Information"
    }
},
"AzureAppServicesFile": {
    "IncludeScopes": true,
    "LogLevel": {
        "Default": "Warning"
    }
},
"AzureAppServicesBlob": {
    "IncludeScopes": true,
    "LogLevel": {
        "Microsoft": "Information"
    }
},
"ApplicationInsights": {
    "LogLevel": {
        "Default": "Information"
    }
}
}

